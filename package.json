    "gulp-minify-css": "~0.3.11",
    "gulp-ng-annotate": "~0.3.3",
    "gulp-ng-html2js": "~0.1.8",
    "gulp-sourcemaps": "~1.2.4",
    "gulp-uglify": "~1.0.1",
