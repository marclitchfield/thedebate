{
  "name": "the-debate",
  "version": "0.0.1",
  "description": "The Debate: Web Site\r =========",
  "main": "dist/server.js",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marclitchfield/thedebate-web.git"
  },
  "scripts": {
    "setup": "npm install -g phantomjs bower gulp supervisor protractor@1.3.1",
    "clean": "gulp clean",
