    "good-console": "~4.0.0",
    "mongoose": "~3.8.21",
    "mongodb": "~1.4.29"
  },
  "devDependencies": {
    "del": "~0.1.3",
    "gulp": "^3.9.0",
    "gulp-changed": "~1.0.0",
    "gulp-concat": "~2.4.1",
    "gulp-jshint": "~1.8.5",
    "gulp-less": "~1.3.6",
    "gulp-livereload": "~2.1.1",
    "gulp-minify-css": "~0.3.11",
