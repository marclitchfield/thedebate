{
  "name": "the-debate",
  "version": "0.0.1",
  "description": "The Debate: Web Site\r =========",
  "main": "dist/server.js",
  "private": true,
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "repository": {
    "type": "git",
